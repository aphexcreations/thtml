{
  "name": "thtml",
  "version": "2.2.3",
  "description": "Typesafe HTML templating for NodeJS",
  "main": "build/index.js",
  "repository": "git@github.com:felipeochoa/thtml.git",
  "author": "Felipe Ochoa",
  "license": "MIT",
  "private": false,
  "scripts": {
    "build": "tsc -p tsconfig.json",
    "test": "jest"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "jsx",
      "ts",
      "tsx"
    ],
    "transform": {
      "\\.[jt]sx?$": "ts-jest"
    },
    "testEnvironment": "node",
    "testPathIgnorePatterns": [
      "/node_modules/",
      "<rootDir>/build/"
    ]
  },
  "devDependencies": {
    "@types/jest": "^26.0.20",
    "jest": "^26.6.3",
    "ts-jest": "^26.4.4",
    "typescript": "^4.1.3"
  },
  "dependencies": {}
}
